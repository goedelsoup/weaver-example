#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/goedelsoup/weaver-example/packages/registry-importer/dist/node_modules:/home/goedelsoup/weaver-example/packages/registry-importer/node_modules:/home/goedelsoup/weaver-example/packages/node_modules:/home/goedelsoup/weaver-example/node_modules:/home/goedelsoup/node_modules:/home/node_modules:/node_modules:/home/goedelsoup/weaver-example/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/goedelsoup/weaver-example/packages/registry-importer/dist/node_modules:/home/goedelsoup/weaver-example/packages/registry-importer/node_modules:/home/goedelsoup/weaver-example/packages/node_modules:/home/goedelsoup/weaver-example/node_modules:/home/goedelsoup/node_modules:/home/node_modules:/node_modules:/home/goedelsoup/weaver-example/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@internal/registry-importer/dist/import.mjs" "$@"
else
  exec node  "$basedir/../@internal/registry-importer/dist/import.mjs" "$@"
fi
